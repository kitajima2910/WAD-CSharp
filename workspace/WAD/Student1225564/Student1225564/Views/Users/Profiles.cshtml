@model Student1225564.Models.User
@{
    ViewData["Title"] = "Profiles";
}

<h1>User profiles</h1>

<table>
    <tr>
        <th>Fullname:</th>
        <td style="text-indent: 16px;">@Html.DisplayFor(model => model.Fullname)</td>
    </tr>
    <tr>
        <th>Username:</th>
        <td style="text-indent: 16px;">@Html.DisplayFor(model => model.Username)</td>
    </tr>
    <tr>
        <th>Password:</th>
        <td style="text-indent: 16px;">@Html.DisplayFor(model => model.Password)</td>
    </tr>
    <tr>
        <th>Address:</th>
        <td style="text-indent: 16px;">@Html.DisplayFor(model => model.Address)</td>
    </tr>
    <tr>
        <th>Email:</th>
        <td style="text-indent: 16px;">@Html.DisplayFor(model => model.Email)</td>
    </tr>
</table>
